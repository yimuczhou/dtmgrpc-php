{
    "name": "yimuzhou/dtmgrpc-php",
    "description": "dtm php grpc sdk",
    "type": "library",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "psr/log": "^1.0",
        "google/protobuf": "^v3.17.0",
        "grpc/grpc": "^v1.38.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "monolog/monolog": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "DtmGrpc\\": "src/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DtmGrpc\\Tests\\": "tests/"
        }
    }
}
